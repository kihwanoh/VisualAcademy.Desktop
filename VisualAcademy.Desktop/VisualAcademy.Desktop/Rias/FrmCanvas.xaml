<Window x:Class="VisualAcademy.Desktop.Rias.FrmCanvas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualAcademy.Desktop.Rias"
        mc:Ignorable="d"
        Title="FrmCanvas" Height="450" Width="800" d:DesignHeight="300" d:DesignWidth="400">
    <Grid>
        <Canvas Width="200" Height="200" Background="Yellow">
            <Rectangle Width="50" Height="50" Stroke="Red" StrokeThickness="2"
                   Canvas.Left="50" Canvas.Top="50"
                   ></Rectangle>
            <Ellipse Width="50" Height="50" Stroke="Blue" StrokeThickness="1"
                 Canvas.Left="100" Canvas.Top="100"
                 ></Ellipse>
            <Button Content="안녕" Canvas.Left="50" Canvas.Top="150"></Button>
        </Canvas>
    </Grid>
</Window>
